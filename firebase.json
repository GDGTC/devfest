{
    "database": {
        "rules": "database.rules.json"
    },
    "hosting": {
        "public": "dist",
        "rewrites": [
            { "source": "/sitemap.txt", "function": "generateSitemap" },
            {
                "source": "/worker-basic.min.js",
                "destination": "/ngsw-worker.js"
            },
            {
                "source": "/worker-basic.js",
                "destination": "/ngsw-worker.js"
            },
            {
                "source": "**",
                "destination": "/index.html"
            }
        ],
        "headers": [
            {
                "source": "/",
                "headers": [
                    {
                        "key": "Link",
                        "value":
                            "</inline.74706af40d07a531c707.bundle.js>;rel=preload;as=script,</main.591aa07753f4606a3a92.bundle.js>;rel=preload;as=script,</polyfills.d8d030261d4351ef3651.bundle.js>;rel=preload;as=script,</styles.31b6030277c73d00fc5d.bundle.css>;rel=preload;as=style,"
                    }
                ]
            }
        ]
    },
    "functions": {
        "predeploy": "npm --prefix functions run build"
    }
}
